<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ServletPinturas</servlet-name>
        <servlet-class>com.pintura.campusjalpa.ServletPinturas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletTecnicas</servlet-name>
        <servlet-class>com.pintura.campusjalpa.ServletTecnicas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPinturas</servlet-name>
        <url-pattern>/ServletPinturas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletTecnicas</servlet-name>
        <url-pattern>/ServletTecnicas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <!--PÃ¡gina de bienvenida-->
    <welcome-file-list>
        <!--<welcome-file>ServletPinturas</welcome-file>-->
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
